{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "21.0.0",
  "compilerOptions": {
    "lib": ["es2023"],
    "module": "node16",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node16",
    "typeRoots": ["./src/@types", "./node_modules/@types"]
  }
}
